  },
  "devDependencies": {
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.8.2",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "supertest": "^7.0.0"
  }
}
